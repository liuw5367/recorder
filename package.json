{
  "name": "@lw6/recorder",
  "type": "module",
  "version": "0.1.1",
  "packageManager": "pnpm@8.15.3",
  "description": "A simple browser recording tool.",
  "author": "liuw5367 <liuw5367@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/liuw5367/recorder#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liuw5367/recorder.git"
  },
  "bugs": "https://github.com/liuw5367/recorder/issues",
  "keywords": [
    "recorder",
    "audio",
    "browser",
    "pcm",
    "wav",
    "typescript",
    "WebAudioAPI"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "release": "bumpp && npm publish",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@types/node": "^20.11.19",
    "bumpp": "^9.3.0",
    "eslint": "^8.56.0",
    "eslint-plugin-format": "^0.1.0",
    "lint-staged": "^15.2.2",
    "pnpm": "^8.15.3",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "unocss": "^0.58.6",
    "vite": "^5.1.4",
    "vitest": "^1.3.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
